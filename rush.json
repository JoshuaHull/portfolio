{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.93.2",
  "pnpmVersion": "6.7.1",
  "nodeSupportedVersionRange": ">=16.13.0 <17.0.0",
  "gitPolicy": {
    "versionBumpCommitMessage": "chore: bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "chore: update changelogs [skip ci]"
  },
  "repository": {
    "url": "https://github.com/JoshuaHull/portfolio",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "allowedProjectTags": [
    "apps",
    "libs"
  ],
  "projects": [
    {
      "packageName": "homepage",
      "projectFolder": "apps/homepage"
    },
    {
      "packageName": "skills",
      "projectFolder": "apps/skills"
    },
    {
      "packageName": "rollup-plugin-content",
      "projectFolder": "libs/rollup-plugin-content"
    },
    {
      "packageName": "rollup-plugin-count",
      "projectFolder": "libs/rollup-plugin-count"
    },
    {
      "packageName": "rollup-plugin-html-minify",
      "projectFolder": "libs/rollup-plugin-html-minify"
    },
    {
      "packageName": "csharp-lexer",
      "projectFolder": "libs/csharp-lexer"
    },
    {
      "packageName": "hero-icon-resolver",
      "projectFolder": "libs/hero-icon-resolver",
      "shouldPublish": true
    },
    {
      "packageName": "re-lex-ation",
      "projectFolder": "libs/re-lex-ation"
    },
    {
      "packageName": "highlighter",
      "projectFolder": "libs/highlighter"
    },
    {
      "packageName": "@types/highlighter",
      "projectFolder": "types/highlighter"
    },
    {
      "packageName": "typescript-lexer",
      "projectFolder": "libs/typescript-lexer"
    },
    {
      "packageName": "vue-lexer",
      "projectFolder": "libs/vue-lexer"
    },
    {
      "packageName": "vite-plugin-replace",
      "projectFolder": "libs/vite-plugin-replace"
    },
    {
      "packageName": "use-vanishing-value",
      "projectFolder": "composables/use-vanishing-value"
    },
    {
      "packageName": "use-vanishing-values",
      "projectFolder": "composables/use-vanishing-values"
    }
  ]
}
